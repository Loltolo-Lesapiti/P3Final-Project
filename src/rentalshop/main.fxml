      </ToolBar>
      <Label id="TransactionId" fx:id="transaction" layoutX="336.0" layoutY="40.0" prefHeight="17.0" prefWidth="93.0" text="Transaction" underline="true">
         <font>
            <Font name="Arial Bold Italic" size="12.0" />
         </font>
      </Label>
      <Label id="titleid" fx:id="title" layoutX="31.0" layoutY="31.0" text="TAAS RENTALS " textFill="#13c910">
